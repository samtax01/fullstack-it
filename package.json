{
  "name": "fullstack-it",
  "description": "A fullstack framework for building web applications",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.11.0"
  },
  "scripts": {
    "start:backend": "node --env-file=.env.production --import=tsx src/backend/index.ts",
    "dev:frontend": "vite --mode development",
    "dev:backend": "node --env-file=.env --import=tsx --watch-path=./src/backend src/backend/index.ts",
    "start": "npm run prisma:generate && npm run start:backend",
    "dev": "concurrently 'npm run dev:frontend' 'npm run dev:backend'",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.js"
  },
  "dependencies": {
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/node": "^20.11.19",
    "best-effort-json-parser": "^1.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "entities": "^4.5.0",
    "express": "^4.18.2",
    "json-like-parse": "^1.0.3",
    "luxon": "^3.4.4",
    "query-string": "^8.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-resizable-layout": "^0.7.2",
    "react-router-dom": "^6.22.0",
    "styled-components": "^6.1.8",
    "sweetalert2": "^11.6.13",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.7.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/luxon": "^3.4.2",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^5.12.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.2.2"
  }
}
